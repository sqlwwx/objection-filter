    }
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "knex": "^0.14.4",
    "mocha": "^3.5.0",
    "mysql": "^2.16.0",
    "nyc": "^11.1.0",
    "objection": "github:sqlwwx/objection.js#e4dff75",
